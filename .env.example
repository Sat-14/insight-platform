# ============================================================================
# AMEP Frontend Environment Configuration
# Copy this file to .env and update with your actual values
# ============================================================================

# Backend API URL
VITE_API_URL=http://localhost:5000

# WebSocket URL (if different from API URL)
VITE_WS_URL=http://localhost:5000

# Environment
VITE_ENV=development

# Feature Flags
VITE_ENABLE_LIVE_POLLING=true
VITE_ENABLE_SOFT_SKILLS=true
VITE_ENABLE_PBL_WORKSPACE=true
VITE_ENABLE_TEMPLATE_LIBRARY=true
